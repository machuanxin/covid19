<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sgg.covid19.dao.HospitalDao">
    <select id="queryHospital" resultType="com.sgg.covid19.entity.Hospital">
        SELECT hospital_id,hospital_name,longitude,latitude,fever_clinic,designated_medical_institution,
        designated_detection_institution
        FROM tb_hospital
    </select>
</mapper>
