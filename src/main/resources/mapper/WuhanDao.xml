<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sgg.covid19.dao.WuhanDao">
    <select id="queryWuhan" resultType="com.sgg.covid19.entity.Wuhan">
        SELECT date,new_case,total_case,new_death,total_death,new_cure,total_cure,
        death_rate,cure_rate
        FROM tb_wuhan
    </select>
</mapper>
